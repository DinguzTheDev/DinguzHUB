local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Dinguz HUB | Stone Farm Simulator! â›", "Serpent")

--Info
local Tab = Window:NewTab("â” Info")
local Section = Tab:NewSection("Welcome to DINGUZ HUB!")
Section:NewLabel("Made by CG Studio / Dinguz ğŸ˜")
Section:NewLabel("We activly working on a new updates!")
Section:NewLabel("Credits: Dinguz#9873")
