local Tab = Window:NewTab("ü•± Misc")
local Section = Tab:NewSection("üíæ Server üíæ")
Section:NewButton("Rejoin Server", "Rejoining server.", function()	
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
 
local Rejoin = coroutine.create(function()
    local Success, ErrorMessage = pcall(function()
        TeleportService:Teleport(game.PlaceId, LocalPlayer)
    end)
 
    if ErrorMessage and not Success then
        warn(ErrorMessage)
    end
end)
 
coroutine.resume(Rejoin)
end)
local Tab = Window:NewTab("‚öô Settings")
local Section = Tab:NewSection("üñ• UI üñ•")
Section:NewKeybind("Open/Close UI", "Change it if you want.", Enum.KeyCode.RightShift, function()
	Library:ToggleUI()
end)
